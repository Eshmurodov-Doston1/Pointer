<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F7F7FC"
    android:transitionName="view"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.student.MyCourceFragment">
    <TextView
        android:id="@+id/name_page"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.03"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintRight_toRightOf="parent"
        android:maxLines="1"
        android:textSize="@dimen/_19sdp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:text="Mening kurslarim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.viewpager2.widget.ViewPager2
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        app:layout_constraintTop_toBottomOf="@id/name_page"
        android:id="@+id/cource_viewPager"
        app:layout_constraintVertical_bias="0.05"
        android:layout_width="match_parent"
        app:layout_constraintHeight_percent="0.9"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_height="wrap_content"/>
</androidx.constraintlayout.widget.ConstraintLayout>